{
    "compilerOptions":  {
                            "target":  "es2015",
                            "lib":  [
                                        "dom",
                                        "dom.iterable",
                                        "esnext"
                                    ],
                            "allowJs":  true,
                            "skipLibCheck":  true,
                            "strict":  false,
                            "forceConsistentCasingInFileNames":  true,
                            "noEmit":  true,
                            "esModuleInterop":  true,
                            "module":  "esnext",
                            "moduleResolution":  "node",
                            "resolveJsonModule":  true,
                            "isolatedModules":  true,
                            "jsx":  "preserve",
                            "incremental":  true,
                            "baseUrl":  ".",
                            "paths":  {
                                          "@/*":  [
                                                      "./*"
                                                  ]
                                      },
                            "plugins":  [
                                            {
                                                "name":  "next"
                                            }
                                        ]
                        },
    "include":  [
                    "next-env.d.ts",
                    "**/*.ts",
                    "**/*.tsx",
                    "src/**/*",
                    ".next/types/**/*.ts"
                ],
    "exclude":  [
                    "node_modules",
                    "**/*backup*",
                    "**/*phase*",
                    "**/*.backup",
                    "**/*backup-*",
                    "**/*backup_*",
                    "**/*.backup.*",
                    "**/node_modules",
                    "**/.next",
                    "**/dist",
                    "**/build"
                ]
}
